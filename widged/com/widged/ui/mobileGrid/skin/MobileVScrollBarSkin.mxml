<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009" minWidth="15" minHeight="35" 
             alpha.disabled="0.5" alpha.inactive="0.5" >
    
    <fx:Metadata>
        <![CDATA[ 
        [HostComponent("spark.components.VScrollBar")]
        ]]>
    </fx:Metadata> 
    
    <s:states>
        <s:State name="normal" />
        <s:State name="disabled" />
        <s:State name="inactive" />
    </s:states>
    
    <s:Button id="track" top="16" bottom="15" height="54"
              focusEnabled="false"
              skinClass="embed.skins.MobileVScrollBarTrackSkin" />
    
    <s:Button id="thumb" 
              focusEnabled="false" visible.inactive="false"
              skinClass="embed.skins.MobileVScrollBarTumbSkin" />
    
</s:SparkSkin>
